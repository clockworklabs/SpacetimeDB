<Project>
  <!-- Skip Windows App SDK PRI generation that requires VS build tools -->
  <PropertyGroup>
    <_MrtCoreGeneratePriEnabled>false</_MrtCoreGeneratePriEnabled>
  </PropertyGroup>
  
  <!-- Override the problematic target to do nothing -->
  <Target Name="_ExpandPriContent" />
  <Target Name="_MrtCoreGeneratePri" />
  <Target Name="_MrtCoreIndexResources" />
</Project>
